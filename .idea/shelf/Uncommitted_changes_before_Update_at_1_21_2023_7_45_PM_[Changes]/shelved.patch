content\">\r\n <fieldset>\r\n      <p>Offer:</p>\r\n      <input type=\"radio\" id=\"sellerf\" name=\"offer\" value=\"Seller fulfilled\">\r\n      <label for=\"True\">Seller fulfilled</label>\r\n      <br>\r\n      <input type=\"radio\" id=\"Ebf\" name=\"offer\" value=\"E-bazar fulfilled\">\r\n      <label for=\"True\">E-bazar fulfilled</label>\r\n      <br><br>\r\n     <input type=\"checkbox\" id=\"isb2b\" name=\"isb2b\" value=\"1\" onclick=\"Enableb2b('isb2b')\">\r\n      <label for=\"isbrand\">Business offer</label>\r\n     <br><br>\r\n     <p id=\"b2binfo\" hidden>Maximum 3 batches allowed<p>\r\n     <br><br>\r\n\r\n     <label id=\"b2boffamol\" for=\"b2boffamo\" hidden>Amount in batch(range):</label>&nbsp;&nbsp;\r\n        <br>\r\n      <input type=\"text\" id=\"b2boffamo\" name=\"b2boffamo\" hidden>\r\n     <br>\r\n     <label id=\"b2boffpril\" for=\"b2boffpri\" hidden>Price per item:</label>&nbsp;&nbsp;\r\n        <br>\r\n      <input type=\"text\" id=\"b2boffpri\" name=\"b2boffpri\" hidden >\r\n        <br><br>\r\n\r\n     <label id=\"b2boffamol1\" for=\"b2boffamo1\" hidden>Amount in batch(range):</label>&nbsp;&nbsp;\r\n        <br>\r\n      <input type=\"text\" id=\"b2boffamo1\" name=\"b2boffamo1\" hidden>\r\n     <br>\r\n     <label id=\"b2boffpril1\" for=\"b2boffpri1\" hidden>Price per item:</label>&nbsp;&nbsp;\r\n        <br>\r\n      <input type=\"text\" id=\"b2boffpri1\" name=\"b2boffpri1\" hidden>\r\n        <br><br>\r\n\r\n     <label id=\"b2boffamol2\" for=\"b2boffamo2\" hidden>Amount in batch(range):</label>&nbsp;&nbsp;\r\n        <br>\r\n      <input type=\"text\" id=\"b2boffamo2\" name=\"b2boffamo2\" hidden>\r\n     <br>\r\n     <label id=\"b2boffpril2\" for=\"b2boffpri2\" hidden>Price per item:</label>&nbsp;&nbsp;\r\n        <br>\r\n      <input type=\"text\" id=\"b2boffpri2\" name=\"b2boffpri2\" hidden>\r\n\r\n\r\n\r\n    </fieldset>\r\n</div>\r\n\r\n<div id=\"Images\" class=\"tabcontent\">\r\n  <fieldset>\r\n      <p>Upload your product images